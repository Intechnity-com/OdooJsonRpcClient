<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Patryk Bujalla</Authors>
    <Company>Porta Capena</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/patricoos/PortaCapena.OdooJsonRpcClient</PackageProjectUrl>
    <PackageIcon>ic_project.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/patricoos/PortaCapena.OdooJsonRpcClient</RepositoryUrl>
    <Copyright>Porta Capena</Copyright>
    <PackageTags>Odoo JsonRpc Client</PackageTags>
    <Description>JsonRpc Odoo Client</Description>
    <Version>1.0.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="nuget.exe" />
    <None Remove="PortaCapena.OdooJsonRpcClient.v3.ncrunchproject" />
    <None Include="ic_project.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
